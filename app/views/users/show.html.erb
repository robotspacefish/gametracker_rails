<%= display_title("#{@user.username}'s Home") %>

<h2>Recently Added</h2>
<% @user.games.recently_added.each do |game| %>
  <div>
    <%= game.title %><br>
  </div>
<% end %>


<h2>Currently Playing</h2>

<% @user.currently_playing.each do |game| %>
  <div>
    <%= game.title %><br>
  </div>
  <br>
<% end %>