<h1>Edit Note</h1>

<%= form_for([@game, @note]) do |f| %>
  <%= f.label :title %><br>
  <%= f.text_field :title %>
  <br>
  <%= f.label :content %><br>
  <%= f.text_area :content %>
  <br><br>

  <%= f.label :completed, "Completed?" %>
  <%= f.check_box :completed %><br>

  <br><br>
  <%= f.submit %>
<% end %>