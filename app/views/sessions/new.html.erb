<%= display_title("Log In") %>

<%= render 'layouts/errors', { object: @user } %>

<%= form_for(@user, url: login_path ) do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %><br>
  <%= f.label :password %>
  <%= f.password_field :password %><br>

  <%= f.submit "Log In", class: submit_button_classes %>
<% end %>